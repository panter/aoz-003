.row
  .col-xs-12
    h1= t_title(:new)

= simple_form_for(@volunteer, url: volunteers_path) do |f|
  = render 'volunteers/form', f: f
  = render 'schedules/form', f: f, participant: @volunteer
  = f.button :submit, class: 'btn btn-success'

.row
  .col-xs-12
    = link_to t('back'), volunteers_path, class: 'btn btn-default'
